<resources>
	<declare-styleable name="MyListView">
		<attr name="maxThrowVelocity" format="integer"/>
		<attr name="vibrateOnTearOff" format="integer"/>
	</declare-styleable>
	<declare-styleable name="SlideLeftButton">
		<attr name="maxThrowVelocity"/>
		<attr name="waitForSlide" format="integer"/>
	</declare-styleable>
	<declare-styleable name="SliderButton">
		<attr name="valuesList" format="string"/>
		<attr name="valueSuffix" format="string"/>
		<attr name="separator" format="string"/>
		<attr name="defaultValueIndex" format="integer"/>
	</declare-styleable>
	<declare-styleable name="TodoItemView">
		<attr name="progress0Color" format="color"/>
		<attr name="progress100Color" format="color"/>
		<attr name="dueDateColor" format="color"/>
		<attr name="todayDueDateColor" format="color"/>
		<attr name="expiredDueDateColor" format="color"/>
		<attr name="notesDrawable" format="reference"/>
		<attr name="prioStripeWidth" format="dimension"/>
		<attr name="prio0Color" format="color"/> <!-- 0 = item done -->
		<attr name="prio1Color" format="color"/>
		<attr name="prio2Color" format="color"/>
		<attr name="prio3Color" format="color"/>
		<attr name="prio4Color" format="color"/>
		<attr name="prio5Color" format="color"/>
	</declare-styleable>
	<declare-styleable name="WidgetTextView">
		<attr name="prioStripeWidth"/>
		<attr name="prio0Color"/>
		<attr name="prio1Color"/>
		<attr name="prio2Color"/>
		<attr name="prio3Color"/>
		<attr name="prio4Color"/>
		<attr name="prio5Color"/>
	</declare-styleable>
	<declare-styleable name="AnimatedVisibilityButton">
		<attr name="inAnim" format="reference"/>
		<attr name="outAnim" format="reference"/>
	</declare-styleable>
</resources>